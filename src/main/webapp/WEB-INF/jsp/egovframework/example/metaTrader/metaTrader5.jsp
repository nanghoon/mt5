  <script>
	  $(document).ready(function () {
		    // 초기 상태 설정: Windows 리스트만 보이게 하고, Windows 아이콘에 on 클래스 추가
		    $(".txt-list").hide(); // 모든 리스트 숨김
		    $(".txt-list.windows").show(); // Windows 리스트만 표시
		    $(".operating-system-icon").removeClass("on"); // 모든 아이콘에서 on 제거
		    $(".operating-system-icon").first().addClass("on"); // 첫 번째 아이콘에 on 추가
	
		    // 클릭 이벤트 처리
		    $(".operating-system-icon").on("click", function () {
		        const index = $(this).index(); // 클릭된 아이콘의 순서 가져오기
	
		        // 모든 아이콘에서 on 클래스 제거
		        $(".operating-system-icon").removeClass("on");
	
		        // 클릭된 아이콘에 on 클래스 추가
		        $(this).addClass("on");
	
		        // 모든 txt-list 숨김
		        $(".txt-list").hide();
	
		        // 클릭된 아이콘에 대응하는 리스트 표시
		        $(".txt-list").eq(index).fadeIn();
		    });
		});
  </script>
